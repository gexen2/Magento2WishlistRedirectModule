<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="crealevant" translate="label" sortOrder="10">
            <label>Crealevant</label>
        </tab>
        <section id="crealevantWishlist" translate="label" sortOrder="130" showInDefault="1" showInWebsite="1" showInStore="1">
            <class>separator-top</class>
            <label>Wishlist</label>
            <tab>crealevant</tab>
            <resource>Crealevant_AddToWishlistNotRedirect::wishlist_no_redirect</resource>
            <group id="crealevantGeneral" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
                <label>General Configuration</label>
                <field id="wishlistRedirect" translate="label" type="select" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1" canRestore="1">
                  <label>Wishlist Redirect</label>
                  <source_model>Crealevant\AddToWishlistNotRedirect\Model\Config\Source\Options</source_model>
                </field>
            </group>
        </section>
    </system>
</config>
